      "react-masonry-component": "^6.2.1",
      "react-redux": "^7.2.1",
      "react-router-dom": "^5.2.0",
      "redux": "^4.0.5",
      "redux-thunk": "^2.3.0",
      "semantic-ui-css": "^2.4.1",
